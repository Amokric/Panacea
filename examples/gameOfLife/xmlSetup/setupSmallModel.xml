            <!-- Agent 8 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="2"/>
                        <coordinate name="y" value="1"/>
                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 9 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="2"/>
