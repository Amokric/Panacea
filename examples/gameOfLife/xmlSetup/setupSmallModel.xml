                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 3 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="0"/>
                        <coordinate name="y" value="2"/>
                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 4 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="1"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="1"/>
                        <coordinate name="y" value="0"/>
                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 5 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="1"/>
                        <coordinate name="y" value="1"/>
                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 6 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
                        <coordinate name="x" value="1"/>
                        <coordinate name="y" value="2"/>
                    </gridPosition>

                </gridPositions>

            </agent>

            <!-- Agent 7 -->
            <agent type="GOLCell">

                <parameters>

                    <paramter name="radius" value="1"/>
                    <parameter name="state" value="0"/>

                </parameters>

                <gridPositions>

                    <gridPosition grid="golgrid">
